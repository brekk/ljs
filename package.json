{
  "resolutions": {
    "**/event-stream": "^4.0.1"
  },
  "name": "ljs2",
  "version": " 1.0.3",
  "description": "Literate programming in the command-line",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/brekk/ljs2.git"
  },
  "bin": {
    "ljs2": "ljs2.js"
  },
  "author": "brekk",
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.6",
    "@babel/plugin-transform-destructuring": "^7.1.3",
    "@babel/preset-env": "7.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "depcheck": "^0.6.11",
    "documentation": "^8.1.2",
    "docusaurus": "1.5.0",
    "eslint": "5.7.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-babel": "5.2.1",
    "eslint-plugin-delorean": "^0.0.1",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "execa": "^1.0.0",
    "husky": "1.1.2",
    "jest": "^23.6.0",
    "nps": "^5.9.3",
    "nps-utils": "^1.7.0",
    "prettier": "1.14.3",
    "prettier-eslint": "^8.8.2",
    "rollup": "^0.66.6",
    "rollup-plugin-buble": "0.19.4",
    "rollup-plugin-cleanup": "3.0.0",
    "rollup-plugin-cli": "^0.1.5",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-progress": "^0.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "nps care"
    }
  },
  "keywords": [
    "terminal",
    "cli",
    "tool",
    "js-in-terminal",
    "js in terminal"
  ],
  "clinton": {
    "rules": {
      "cli": true,
      "gitignore": true,
      "license": true,
      "keywords": true,
      "no-dup-keywords": true,
      "no-empty-keywords": true,
      "no-git-merge-conflict": true,
      "pkg-dependency-order": true,
      "pkg-main": true,
      "pkg-user-order": true,
      "readme": true,
      "test-script": true,
      "pkg-name": true,
      "valid-version": true
    }
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePathIgnorePatterns": [
      "lib"
    ],
    "coveragePathIgnorePatterns": [],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testMatch": [
      "**/*.spec.(jsx|js)"
    ]
  },
  "dependencies": {
    "entrust": "^0.0.21",
    "esprima": "^4.0.1",
    "f-utility": "^3.5.8",
    "fluture": "^10.3.0",
    "glob": "^7.1.3",
    "lodash": "^4.17.11",
    "ramda": "^0.26.1",
    "sywac": "^1.2.1",
    "through2": "^3.0.0",
    "xtrace": "^0.1.10",
    "yargs-parser": "^11.1.1"
  },
  "scripts": {
    "dependencies": "nps dependencies",
    "readme": "nps readme",
    "lint": "nps lint",
    "test": "nps test",
    "docs": "nps docs",
    "bundle": "nps bundle",
    "build": "nps generate",
    "prepublishOnly": "npm run build"
  },
  "bugs": {
    "url": "https://gitlab.com/brekk/snang/issues"
  },
  "homepage": "https://gitlab.com/brekk/snang#readme",
  "main": "snang.js",
  "directories": {
    "lib": "lib"
  }
}
